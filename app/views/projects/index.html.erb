<% provide(:title, 'All Project') %>
<h1>All Projects</h1>

<ul class="project">
  <% @projects.each do |project| %>
    <li>
      <%= link_to project.project_name, project %>
      <%= project.plan_time%>
      <%= project.real_time%>
      <%= link_to "Edit", edit_project_path(project) %> | 
      <%= link_to "delete", project, method: :delete,
                                  data: { confirm: "You sure?" } %>
    </li>
  <% end %>
</ul>
<%= link_to "Them du an", management_path, class: "btn btn-lg btn-primary" %>